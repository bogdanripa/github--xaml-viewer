{
  "name": "UiPath XAML support in GitHub",
  "version": "1.0.1",
  "description": "Adds support for viewing XAML files in GitHub",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "run_at": "document_end",
      "css": ["myStyles.css"],
      "js": ["xmlFunctions.js", "uia.js", "system.js", "office.js", "contentScript.js"]
    }
  ]
}